      <JFXButton fx:id="btBack" contentDisplay="GRAPHIC_ONLY" layoutX="10.0" layoutY="505.0" onAction="#gameMenu" prefHeight="40.0" prefWidth="40.0" textFill="WHITE">
         <graphic>
            <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
