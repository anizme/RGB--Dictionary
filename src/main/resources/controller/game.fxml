<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="gameContainer" prefHeight="555.0" prefWidth="850.0" style="-fx-background-color: #def3f6;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.panes.GameController">
