      <ImageView fx:id="gameMenuBackground" fitHeight="555.0" fitWidth="850.0" pickOnBounds="true">
         <image>
            <Image url="@../images/gameMenuBackground.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
